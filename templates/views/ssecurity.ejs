      margin-left: -5%;
    }

    .hideMobile {
      display: none;

    }

    .minContent {
      display: none;
    }

    .b2 {
      background: linear-gradient(rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.7));
    }

    .card {
      transition: 0.2s;
    }

    .card:hover {
      box-shadow: 1px 1px 15px black;
      transition: 0.3s;
      position: relative;
      top: -4px;
    }

    .card:hover .card-img-top {
      filter: sepia(0);
      transition: 0.3s all ease-in-out;
    }

    .card-img-top {
      filter: sepia(50%);
      transition: all 0.3s ease-in-out;
    }



    .d-none {
      display: none;
    }

    .socialMeadia {
      position: fixed;
      top: 40%;
      left: 10px;
      z-index: 10000;
      transition: 0.2s all ease-in-out;
    }

    #show:hover {
      transform: scale(1.1);
      transition: 0.2s all ease-in-out;
    }

    .loginFix {
      margin-left: 24px;
    }

    #input {
      border: none;
      border-bottom: 2px solid #3f51b5;
    }

    @media screen and (max-width:1200px) {
      .responsiveSearch {
        display: hidden;
      }

      .minContent {
        display: none;
