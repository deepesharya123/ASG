    }

    .effect {
      transition: all 0.4s;
    }

    .effect:hover {
      transition: all 0.4s;
      margin-top: -5px;
      filter: drop-shadow(1px 1px 10px black);
    }

    .shadowIndigo {
      transition: all 0.4s;
    }

    .shadowIndigo:hover {
      transition: all 0.4s;
      margin-top: -5px;
      filter: drop-shadow(1px 1px 10px #3f51b5);
    }

    .navbar-nav li {
